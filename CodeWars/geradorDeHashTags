import re
def generate_hashtag(text):
    hashTagText=""
    text=text.lower().strip()
    texts=re.split(' |_|-|!|\+|\*|\"',text)
    if len(text)>141:
        return False
    elif text!="":
        for index,txt in enumerate(texts):
            hashTagText+=txt.capitalize()
        return "#"+hashTagText
    else:
        return False
